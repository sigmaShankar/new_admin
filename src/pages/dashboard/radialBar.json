    "securityRadial": {
      "series": [71],
      "options": {
        "chart": {
          "height": 280,
          "type": "radialBar",
          "toolbar": { "show": false }
        },
        "plotOptions": {
          "radialBar": {
            "startAngle": -135,
            "endAngle": 225,
            "hollow": {
              "margin": 0,
              "size": "70%",
